              <stringProp name="script">import org.apache.jmeter.protocol.http.sampler.HTTPSamplerProxy
import org.apache.jmeter.protocol.http.control.HeaderManager
import org.apache.jmeter.protocol.http.control.Header
import groovy.json.JsonSlurper

// Check if IBAN exists
def iban = vars.get(&quot;iban&quot;)

if (!iban || iban == &quot;IBAN_NOT_FOUND&quot; || iban.isEmpty()) {

    log.info(&quot;⚠ IBAN not available, calling Account_UUID API first...&quot;)

    // Get customerUuid
    def customerUuid = vars.get(&quot;customerUuid&quot;)

    if (!customerUuid || customerUuid.isEmpty()) {
        log.error(&quot;❌ ERROR: customerUuid not found! Cannot call Account_UUID API&quot;)
        vars.put(&quot;iban&quot;, &quot;CUSTOMER_UUID_MISSING&quot;)
        return
    }

    log.info(&quot;Using customerUuid: &quot; + customerUuid)

    // Create HTTP Sampler
    def sampler = new HTTPSamplerProxy()
    sampler.setDomain(&quot;qa.yappakistan.com&quot;)
    sampler.setPort(443)
    sampler.setProtocol(&quot;https&quot;)
    sampler.setMethod(&quot;GET&quot;)
    sampler.setPath(&quot;/customer/api/account/customer/&quot; + customerUuid)

    // Headers
    def headerManager = new HeaderManager()
    headerManager.add(new Header(&quot;Content-Type&quot;, &quot;application/json&quot;))
    headerManager.add(new Header(&quot;Accept&quot;, &quot;application/json&quot;))

    // Authorization
    def token = vars.get(&quot;token&quot;)
    if (token &amp;&amp; !token.isEmpty()) {
        headerManager.add(new Header(&quot;Authorization&quot;, &quot;Bearer &quot; + token))
        log.info(&quot;Using authorization token&quot;)
    } else {
        log.error(&quot;❌ Authorization token missing&quot;)
    }

    sampler.setHeaderManager(headerManager)

    // Execute request
    log.info(&quot;Calling URL: https://qa.yappakistan.com/customer/api/account/customer/&quot; + customerUuid)

    def result = sampler.sample()
    def responseCode = result.getResponseCode()
    def response = new String(result.getResponseData(), &quot;UTF-8&quot;)

    log.info(&quot;Account_UUID API Response Code: &quot; + responseCode)
    log.info(&quot;Account_UUID API Response: &quot; + response)

    // Validate response
    if (responseCode != &quot;200&quot;) {
        log.error(&quot;❌ ERROR: Account_UUID API returned error code: &quot; + responseCode)
        vars.put(&quot;iban&quot;, &quot;API_ERROR_&quot; + responseCode)
        return
    }

    // Parse JSON
    try {
        def json = new JsonSlurper().parseText(response)

        if (json?.data instanceof List &amp;&amp; !json.data.isEmpty() &amp;&amp; json.data[0]?.iban) {

            def extractedIban = json.data[0].iban

            vars.put(&quot;iban&quot;, extractedIban)
            vars.put(&quot;accountUuid&quot;, json.data[0]?.uuid ?: &quot;&quot;)
            vars.put(&quot;accountNumber&quot;, json.data[0]?.accountNumber ?: &quot;&quot;)
            vars.put(&quot;accountNickname&quot;, json.data[0]?.accountNickname ?: &quot;&quot;)

            log.info(&quot;✅ SUCCESS: IBAN extracted and stored: &quot; + extractedIban)

        } else {
            log.error(&quot;❌ ERROR: IBAN not found in API response&quot;)
            vars.put(&quot;iban&quot;, &quot;IBAN_NOT_FOUND&quot;)
        }

    } catch (Exception e) {
        log.error(&quot;❌ ERROR: Failed to parse response: &quot; + e.message)
        vars.put(&quot;iban&quot;, &quot;EXTRACTION_FAILED&quot;)
    }

} else {
    log.info(&quot;✅ IBAN already available: &quot; + iban)
}</stringProp>
