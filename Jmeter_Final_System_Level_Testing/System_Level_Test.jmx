              <stringProp name="script">import org.apache.jmeter.protocol.http.sampler.HTTPSamplerProxy
import org.apache.jmeter.protocol.http.control.HeaderManager
import org.apache.jmeter.protocol.http.control.Header
import groovy.json.JsonSlurper

/******************************************************************
 * STEP 1: Check if IBAN already exists
 ******************************************************************/
def iban = vars.get(&quot;iban&quot;)

if (iban &amp;&amp; !iban.isEmpty() &amp;&amp; iban != &quot;IBAN_NOT_FOUND&quot;) {
    log.info(&quot;✅ IBAN already available, skipping Account_UUID API: &quot; + iban)
    return
}

log.info(&quot;⚠ IBAN not found. Fetching IBAN from Account_UUID_for_Add_Beneficiary API...&quot;)

/******************************************************************
 * STEP 2: Get required variables
 ******************************************************************/
def customerUuid = vars.get(&quot;customerUuid&quot;)
def token = vars.get(&quot;token&quot;)

if (!customerUuid || customerUuid.isEmpty()) {
    log.error(&quot;❌ customerUuid is missing. Cannot fetch IBAN.&quot;)
    vars.put(&quot;iban&quot;, &quot;CUSTOMER_UUID_MISSING&quot;)
    return
}

if (!token || token.isEmpty()) {
    log.error(&quot;❌ Authorization token missing.&quot;)
    vars.put(&quot;iban&quot;, &quot;TOKEN_MISSING&quot;)
    return
}

/******************************************************************
 * STEP 3: Create Account_UUID API Sampler
 ******************************************************************/
def sampler = new HTTPSamplerProxy()
sampler.setDomain(&quot;qa.yappakistan.com&quot;)
sampler.setPort(443)
sampler.setProtocol(&quot;https&quot;)
sampler.setMethod(&quot;GET&quot;)
sampler.setPath(&quot;/customer/api/account/customer/&quot; + customerUuid)

/******************************************************************
 * STEP 4: Headers
 ******************************************************************/
def headers = new HeaderManager()
headers.add(new Header(&quot;Content-Type&quot;, &quot;application/json&quot;))
headers.add(new Header(&quot;Accept&quot;, &quot;application/json&quot;))
headers.add(new Header(&quot;Authorization&quot;, &quot;Bearer &quot; + token))
sampler.setHeaderManager(headers)

/******************************************************************
 * STEP 5: Execute API
 ******************************************************************/
log.info(&quot;➡ Calling Account_UUID API for customerUuid: &quot; + customerUuid)

def result = sampler.sample()
def responseCode = result.getResponseCode()
def responseBody = new String(result.getResponseData(), &quot;UTF-8&quot;)

log.info(&quot;Account_UUID Response Code: &quot; + responseCode)
log.info(&quot;Account_UUID Response Body: &quot; + responseBody)

/******************************************************************
 * STEP 6: Validate Response
 ******************************************************************/
if (responseCode != &quot;200&quot;) {
    log.error(&quot;❌ Account_UUID API failed with code: &quot; + responseCode)
    vars.put(&quot;iban&quot;, &quot;API_ERROR_&quot; + responseCode)
    return
}

/******************************************************************
 * STEP 7: Parse JSON &amp; Extract IBAN
 ******************************************************************/
try {
    def json = new JsonSlurper().parseText(responseBody)

    if (json?.data instanceof List &amp;&amp; !json.data.isEmpty() &amp;&amp; json.data[0]?.iban) {

        def extractedIban = json.data[0].iban

        vars.put(&quot;iban&quot;, extractedIban)
        vars.put(&quot;accountUuid&quot;, json.data[0]?.uuid ?: &quot;&quot;)
        vars.put(&quot;accountNumber&quot;, json.data[0]?.accountNumber ?: &quot;&quot;)
        vars.put(&quot;accountNickname&quot;, json.data[0]?.accountNickname ?: &quot;&quot;)

        log.info(&quot;✅ IBAN successfully fetched and stored: &quot; + extractedIban)

    } else {
        log.error(&quot;❌ IBAN not present in Account_UUID response&quot;)
        vars.put(&quot;iban&quot;, &quot;IBAN_NOT_FOUND&quot;)
    }

} catch (Exception e) {
    log.error(&quot;❌ JSON parsing failed: &quot; + e.message)
    vars.put(&quot;iban&quot;, &quot;EXTRACTION_FAILED&quot;)
}</stringProp>
